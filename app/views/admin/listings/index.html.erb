<article>

  <h1 id="title">All Listings</h1>

  <div class="demo-grid-1 mdl-grid">

    <% @listings.each do |listing| %>

        <div class="mdl-cell mdl-cell--3-col"><div id="admin-whole-card" class="mdl-card mdl-shadow--2dp demo-card-wide">

          <div class= "mdl-card__title">
            <h3 class="mdl-card__title-text"><%= listing.name %></h3>
          </div>
          <div class="listings-card">
            <%= image_tag listing.image, class: "admin-listing-index-card" %>
          </div>
          <div class="mdl-card__actions mdl-card--border">
            <%= link_to "View Listing", admin_listing_path(listing.id), class: "mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %>
          </div>
        </div></div>

    <% end %>
  </div>
</article>
