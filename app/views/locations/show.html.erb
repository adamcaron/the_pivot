<article>
<h1 id="title">Plan Listings in <%= @listings.first.destination.continent %></h1>

<div class="demo-grid-1 mdl-grid">

  <% @listings.each do |listing| %>

    <div class="mdl-cell mdl-cell--4-col"><div id="whole-card" class="mdl-card mdl-shadow--2dp demo-card-wide">
        <div >
          <img>
        </div>
          <div class="mdl-card__actions mdl-card--border">
            <%= link_to "Listing Information", listing_path(listing), class: "mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %>
          </div>
      </div></div>

    <% end %>
</div>


</article>
